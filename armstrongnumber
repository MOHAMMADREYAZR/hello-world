#include <stdio.h>
int main()
{
    int num;
    int digit;
    int temp;
    int sum=0;
    scanf("%d",&num);
    temp=num;
    while(num!=0)
    {
    digit=num%10;
    sum=sum+(digit*digit*digit);
    num=num/10;
    }
    if(temp==sum)
    {
    printf("%d is an armstronf number",temp);
    }
    else
    printf("%d is not an armstrong number",temp);
    
    
}
